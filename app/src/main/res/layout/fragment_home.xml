<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fragmentHome"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Button
        android:id="@+id/button_new_referral"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:text="New Referral"
        app:layout_constraintStart_toEndOf="@+id/button_dashboard"
        app:layout_constraintTop_toBottomOf="@+id/button_new_visit" />

    <Button
        android:id="@+id/button_sync"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginTop="44dp"
        android:text="Sync"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.293"
        app:layout_constraintStart_toEndOf="@+id/button_client_list"
        app:layout_constraintTop_toBottomOf="@+id/button_new_referral" />

    <Button
        android:id="@+id/button_dashboard"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="216dp"
        android:text="Dashboard"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_add_client3"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="104dp"
        android:text="New Client"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_add_client4"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="104dp"
        android:text="New Client"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_client_list"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="44dp"
        android:text="Client List"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_dashboard" />

    <Button
        android:id="@+id/button_new_client"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="104dp"
        android:text="New Client"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_new_visit"
        android:layout_width="107dp"
        android:layout_height="89dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="108dp"
        android:text="New Visit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/button_new_client"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>