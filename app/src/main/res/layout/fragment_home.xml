<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragmentHome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/listbackground">


    <Button
        android:id="@+id/buttonNewReferral"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="96dp"
        android:text="New Referral"
        app:layout_constraintStart_toEndOf="@+id/buttonDashboard"
        app:layout_constraintTop_toBottomOf="@+id/buttonNewVisit" />

    <Button
        android:id="@+id/buttonSync"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginTop="48dp"
        android:text="Sync"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toEndOf="@+id/buttonClientList"
        app:layout_constraintTop_toBottomOf="@+id/buttonNewReferral" />

    <Button
        android:id="@+id/buttonDashboard"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="292dp"
        android:text="Dashboard"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonAddClient3"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="104dp"
        android:text="New Client"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonAddClient4"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="104dp"
        android:text="New Client"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonClientList"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="64dp"
        android:text="Client List"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonDashboard" />

    <Button
        android:id="@+id/buttonNewClient"
        android:layout_width="105dp"
        android:layout_height="88dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="104dp"
        android:text="New Client"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonNewVisit"
        android:layout_width="107dp"
        android:layout_height="89dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="108dp"
        android:text="New Visit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/buttonNewClient"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dashboard_title"
        android:layout_width="113dp"
        android:layout_height="49dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="4dp"
        android:text="Dashboard"
        android:textColor="#FFFFFF"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:layout_width="409dp"
        android:layout_height="234dp"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="108dp" />

</androidx.constraintlayout.widget.ConstraintLayout>